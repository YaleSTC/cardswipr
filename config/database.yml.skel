_RAILSENV_:
  adapter: mysql2
  encoding: utf8
  reconnect: false
  database: _DB_
  pool: 5
  host: _DBHOST_
  username: _DBUSERNAME_
  password: _DBPASSWORD_
  socket:  /var/lib/mysql/mysql.sock

oracle:
  adapter: oracle_enhanced
  database: _ACSDB_
  username: _ACSDBUSERNAME_
  password: _ACSDBPASSWORD_
  pool: 5
  timeout: 5000
