<%= raw breadcrumbs('home', 'events', 'entry.event', 'entry') %>

<h1><%= @attendance_entry.name %> <small>Details</small></h1>

<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @attendance_entry.first_name %>
</p>

<p>
  <strong>Nickname:</strong>
  <%= @attendance_entry.nickname %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @attendance_entry.last_name %>
</p>

<p>
  <strong>Upi:</strong>
  <%= @attendance_entry.upi %>
</p>

<p>
  <strong>Netid:</strong>
  <%= @attendance_entry.netid %>
</p>

<p>
  <strong>Email:</strong>
  <%= @attendance_entry.email %>
</p>

<p>
  <strong>Telephone:</strong>
  <%= @attendance_entry.telephone %>
</p>

<div class="btn-group">
  <%= link_to yale_directory_link(@attendance_entry.upi), :class => "btn btn-default", :target => "_blank" do %>
    <span class="glyphicon glyphicon-search"></span> Yale Phonebook Entry
  <% end %>
</div>

<br/><br/>

<%= link_to 'Edit', edit_attendance_entry_path(@attendance_entry), class: "btn btn-default" %>
