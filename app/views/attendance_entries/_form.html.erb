<%= simple_form_for([@event, @attendance_entry]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :nickname %>
    <%= f.input :last_name %>
    <%= f.input :upi %>
    <%= f.input :netid %>
    <%= f.input :email %>
    <%= f.input :school %>
    <%= f.input :college_name %>
    <%= f.input :college_abbreviation %>
    <%= f.input :class_year %>
    <%= f.input :telephone %>
    <%= f.input :address %>
    <%= f.hidden_field :event_id, value: @attendance_entry.event.id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
