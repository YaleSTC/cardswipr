<h1>Listing event_attendance_entries</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Nickname</th>
      <th>Last name</th>
      <th>Upi</th>
      <th>Netid</th>
      <th>Email</th>
      <th>School</th>
      <th>College name</th>
      <th>College abbreviation</th>
      <th>Class year</th>
      <th>Telephone</th>
      <th>Address</th>
      <th>Event</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @attendance_entries.each do |attendance_entry| %>
      <tr>
        <td><%= attendance_entry.first_name %></td>
        <td><%= attendance_entry.nickname %></td>
        <td><%= attendance_entry.last_name %></td>
        <td><%= attendance_entry.upi %></td>
        <td><%= attendance_entry.netid %></td>
        <td><%= attendance_entry.email %></td>
        <td><%= attendance_entry.school %></td>
        <td><%= attendance_entry.college_name %></td>
        <td><%= attendance_entry.college_abbreviation %></td>
        <td><%= attendance_entry.class_year %></td>
        <td><%= attendance_entry.telephone %></td>
        <td><%= attendance_entry.address %></td>
        <td><%= attendance_entry.event.title %></td>
        <td><%= link_to 'Show', attendance_entry %></td>
        <td><%= link_to 'Edit', edit_attendance_entry_path(attendance_entry) %></td>
        <td><%= link_to 'Destroy', attendance_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

